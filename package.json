  "bugs": {
    "url": "https://github.com/billylaing/env-get/issues"
  },
  "homepage": "https://github.com/billylaing/env-get#readme",
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-preset-es2015": "^6.16.0",
    "coveralls": "^2.11.14",
    "eslint": "^3.7.1",
    "faucet": "0.0.1",
    "istanbul": "^0.4.5",
    "nyc": "^8.3.0",
    "tap-xunit": "^1.4.0",
    "tape": "^4.6.2"
